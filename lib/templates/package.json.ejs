{
"name": "<%= name %>",
"version": "1.0.0",
"scripts": {
"dev": "vite",
"build": "vite build",
"preview": "vite preview"
<% if(depends.includes('prettier&eslint')){ %>
    ,"lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write ."
<% } %>
},
"dependencies": {
"react": "^17.0.2",
"react-dom": "^17.0.2"
<% if(depends.includes('antd')){ %>
    ,"antd": "^4.18.7"
<% } %>
<% if(depends.includes('autoprefixer&postcss-pxtorem')){ %>
    ,"autoprefixer": "^10.4.2"
    ,"postcss-pxtorem": "^6.0.0"
<% } %>
<% if(depends.includes('axios')){ %>
    ,"axios": "^0.26.0"
<% } %>
<% if(depends.includes('scss')){ %>
    ,"node-sass": "^7.0.1"
    ,"sass": "^1.49.7"
    ,"sass-loader": "^12.4.0"
<% } %>
<% if(depends.includes('react-router-dom-v6')){ %>
    ,"react-router-dom": "6"
<% } %>
<% if(depends.includes('gzip')){ %>
    ,"vite-plugin-compression": "^0.5.1"
<% } %>
<% if(depends.includes('resso')){ %>
    ,"resso": "^0.2.2"
<% } %>

},
"devDependencies": {
"@vitejs/plugin-react": "^1.0.7",
"vite": "^2.7.2"
<% if(depends.includes('prettier&eslint')){ %>
    ,"eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1"
<% } %>
<% if(depends.includes('autoprefixer&postcss-pxtorem')){ %>
    ,"postcss-cli": "^9.1.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-modules": "^4.3.0",
    "postcss-pxtorem": "^6.0.0"
<% } %>

}
}
